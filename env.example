# SEDA Network Configuration
SEDA_NETWORK=testnet
SEDA_RPC_ENDPOINT=https://rpc.testnet.seda.xyz
SEDA_MNEMONIC=your_24_word_mnemonic_phrase_here
SEDA_ORACLE_PROGRAM_ID=your_oracle_program_id_here

# Scheduler Configuration - Optimized for 15-second posting intervals
SCHEDULER_INTERVAL_MS=15000
SCHEDULER_CONTINUOUS=true
SCHEDULER_MAX_RETRIES=3
SCHEDULER_MEMO=SEDA DataRequest

# Cosmos Sequence Coordinator Configuration
COSMOS_POSTING_TIMEOUT_MS=20000
COSMOS_MAX_QUEUE_SIZE=100

# DataRequest Network Configuration
SEDA_DR_TIMEOUT_SECONDS=120
SEDA_DR_POLLING_INTERVAL_SECONDS=5

# Logging Configuration (optional)
LOG_LEVEL=info

# EVM Configuration - Single private key for all networks
EVM_PRIVATE_KEY=your_private_key_for_all_evm_networks_here

# EVM Network Configuration - Base Network Example
BASE_RPC_URL=https://mainnet.base.org
BASE_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
BASE_CHAIN_ID=8453
BASE_GAS_LIMIT=500000
BASE_GAS_PRICE=1000000000
BASE_ENABLED=true

