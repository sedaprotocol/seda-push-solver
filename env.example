# SEDA Network Configuration
SEDA_NETWORK=testnet
SEDA_RPC_ENDPOINT=https://rpc.testnet.seda.xyz
SEDA_MNEMONIC=your_24_word_mnemonic_phrase_here
SEDA_ORACLE_PROGRAM_ID=your_oracle_program_id_here

# Scheduler Configuration - Optimized for 15-second posting intervals
SCHEDULER_INTERVAL_MS=15000
SCHEDULER_CONTINUOUS=true
SCHEDULER_MAX_RETRIES=3
SCHEDULER_MEMO=SEDA DataRequest

# Cosmos Sequence Coordinator Configuration
COSMOS_POSTING_TIMEOUT_MS=20000
COSMOS_MAX_QUEUE_SIZE=100

# DataRequest Network Configuration
SEDA_DR_TIMEOUT_SECONDS=120
SEDA_DR_POLLING_INTERVAL_SECONDS=5

# Logging Configuration (optional)
LOG_LEVEL=info

# EVM Pusher Configuration
EVM_ENABLED_CHAINS=42161,10                    # Arbitrum One, Optimism (comma-separated chain IDs)
EVM_BATCH_POLLING_INTERVAL_MS=10000           # Check for new batches every 10 seconds
EVM_BATCH_WINDOW=10                           # Check last 10 batches each poll
EVM_MAX_BATCH_AGE_MS=3600000                  # Only push batches newer than 1 hour

# EVM Chain Contract Addresses
ARBITRUM_SEDA_CORE_ADDRESS=0x0000000000000000000000000000000000000000
ARBITRUM_SEDA_PROVER_ADDRESS=0x0000000000000000000000000000000000000000
OPTIMISM_SEDA_CORE_ADDRESS=0x0000000000000000000000000000000000000000
OPTIMISM_SEDA_PROVER_ADDRESS=0x0000000000000000000000000000000000000000
BASE_SEDA_CORE_ADDRESS=0x0000000000000000000000000000000000000000
BASE_SEDA_PROVER_ADDRESS=0x0000000000000000000000000000000000000000
POLYGON_SEDA_CORE_ADDRESS=0x0000000000000000000000000000000000000000
POLYGON_SEDA_PROVER_ADDRESS=0x0000000000000000000000000000000000000000

# EVM Performance Configuration
EVM_MAX_PARALLEL_CHAINS=5                     # Push to max 5 chains simultaneously
EVM_MAX_TX_PER_CHAIN=3                        # Max 3 concurrent transactions per chain

# EVM Monitoring Configuration
EVM_ENABLE_METRICS=true                       # Enable detailed metrics collection
EVM_HEALTH_CHECK_INTERVAL_MS=30000           # Health check every 30 seconds
EVM_MIN_SUCCESS_RATE=90                       # Alert if success rate drops below 90%
EVM_MAX_AVG_PUSH_TIME_MS=120000              # Alert if avg push time exceeds 2 minutes
EVM_MAX_CONSECUTIVE_FAILURES=5                # Alert after 5 consecutive failures 